<?xml version="1.0" encoding="iso-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class table="ECH_AFFAIRE_SPARK"
		name="fr.gouv.justice.cassiopee.invariant.affaire.model.EchAffaireEiaSpark"
		lazy="false">

		<!-- PRIMARY KEY -->
		<id name="idAffaire" column="ID_AFFAIRE" type="java.lang.Long">

		</id>
		<property name="affaireJson" type="java.lang.String">
			<column name="AFFAIRE_JSON" sql-type="clob"/>
		</property>
		<property name="mode" type="java.lang.String">
			<column name="MODE_AFFAIRE" />
		</property>
		<property name="codeEtat" type="java.lang.Integer">
			<column name="CODE_ETAT" />
		</property>
			<property name="identifiantJustice" type="java.lang.String">
			<column name="IDENTIFIANT_JUSTICE" />
		</property>

	</class>
</hibernate-mapping>
